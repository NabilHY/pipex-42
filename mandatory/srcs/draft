int	ft_parce(t_comm *comm, char *str)
{
	int i;
	int start;
	int wc;
	int boo;

	i = 0;
	boo = 0;
	wc = 0;
	start = 0;
	while (str[i])
	{
		if (str[i] == ' ' && !boo)
		{
			if (i != start)
			{
				str[i] = '\0';

			}
			start = i + 1;
		}
		else if (str[i] == '\'' && !boo)
		{
			if (i != start)
			{
				str[i] = '\0';
				append(ft_lstnew(ft_strdup(str[start])));
			}
			start = i + 1;
		}
		if (str[i] == '\'')
			boo = 1;
		i++;
	}
}